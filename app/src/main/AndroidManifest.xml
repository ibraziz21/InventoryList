<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.redbravo.inventorylist">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/inventory"
        android:label="@string/app_name"
        android:roundIcon="@drawable/inventory"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StarterPage"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SalesHistory" />
        <activity android:name=".UserReg" />
        <activity android:name=".itemsale" />
        <activity android:name=".loginactivity" />
        <activity android:name=".AddProduct" />
        <activity android:name=".MainActivity"/>


    </application>

</manifest>